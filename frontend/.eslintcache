 flex;\n        align-items: center;\n        img{\n        fill: blue;\n        }\n        h3 {\n        margin-left: 1rem;\n        background: white;\n        color: black;\n        padding: 1rem;\n        }\n    }\n`\n\nconst Video = styled.div`\n    z-index: 2;\n    flex: 1;\n    overflow: hidden;\n`\nexport default ServicesSection\n","/Users/reginaldosantos/dev/cursos/udemy/my-portfolio/frontend/src/styles.js",[],["137","138"],"/Users/reginaldosantos/dev/cursos/udemy/my-portfolio/frontend/src/components/GlobalStyle.js",[],"/Users/reginaldosantos/dev/cursos/udemy/my-portfolio/frontend/src/pages/Portfolio.js",[],"/Users/reginaldosantos/dev/cursos/udemy/my-portfolio/frontend/src/components/Nav.js",["139"],"import React from 'react'\n\n//Styled Components\nimport styled from 'styled-components'\n\nimport { motion } from 'framer-motion'\n\n//Navigation\nimport {Link, useLocation} from 'react-router-dom'\n\nimport Userfront from '@userfront/react'\nimport {LogoutButton} from './auth/Authentication'\n\nimport useWindowDimensions from '../hooks/useWindowDimensions'\n\nUserfront.init('9ny8dvbd')\n\nconst Nav = () => {\n    const { pathname } = useLocation()\n    const { height, width } = useWindowDimensions();\n\n    const menuOptions = {\n        about: '<About Me />',\n        portfolio: '<Portfolio />',\n        contact: '<Contact Me />'\n    }\n\n    return (\n        <StyledNav>\n            <h1><Link to='/'>Reginaldo Santos</Link></h1>\n            <ul>\n                <li>\n                    <Link to='/' >{menuOptions.about}</Link>\n                    <Line\n                        transition={{ duration: 0.75 }}\n                        initial={{ width: '0%' }}\n                        animate={{ width: pathname === '/' ? '50%' : '0%' }}\n                    />\n                </li>\n\n                <li>\n                    <Link to='/portfolio'>{menuOptions.portfolio}</Link>\n                    <Line\n                        transition={{ duration: 0.75 }}\n                        initial={{ width: '0%' }}\n                        animate={{ width: pathname.startsWith('/portfolio') ? '50%' : '0%' }}\n                    />\n                </li>\n\n                <li>\n                    <Link to='/contact'>{menuOptions.contact}</Link>\n                    <Line\n                        transition={{ duration: 0.75 }}\n                        initial={{ width: '0%' }}\n                        animate={{ width: pathname === '/contact' ? '50%' : '0%' }}\n                    />\n                </li>\n\n                {\n                    width > 600 && !Userfront.accessToken() && (\n                        <>\n                            <li>\n                                <Link to='/login'>Login</Link>\n                                <Line\n                                    transition={{ duration: 0.75 }}\n                                    initial={{ width: '0%' }}\n                                    animate={{ width: pathname === '/login' ? '50%' : '0%' }}\n                                />\n                            </li>\n                            <li>\n                                <Link to='/signup'>Cadastro</Link>\n                                <Line\n                                    transition={{ duration: 0.75 }}\n                                    initial={{ width: '0%' }}\n                                    animate={{ width: pathname === '/signup' ? '50%' : '0%' }}\n                                />\n                            </li>\n                        </>\n                    )\n                }\n                {Userfront.accessToken() && <li><LogoutButton /></li>}\n            </ul>\n        </StyledNav>\n    )\n}\n\nconst StyledNav = styled.nav`\n    min-height: 10vh;\n    display: flex;\n    margin: auto;\n    justify-content: space-between;\n    align-items: center;\n    padding: 1rem 10rem;\n    background: #282828;\n    position: sticky;\n    top: 0;\n    z-in